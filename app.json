{
  "expo": {
    "name": "Rewind",
    "slug": "Rewind",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A1A"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#0A0A1A",
      "bundleIdentifier": "com.spoodsjs.rewind",
      "entitlements": {
        "aps-environment": "development"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"],
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.958995611148-52u8fcfk424tf5pg4saqrf5iju9a9dvj"
            ]
          }
        ]
      }
    },
    "notification": {
      "iosDisplayInForeground": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A0A1A"
      },
      "edgeToEdgeEnabled": true,
      "permissions": ["android.permission.CAMERA", "android.permission.RECORD_AUDIO"],
      "package": "com.spoodsjs.rewind"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-splash-screen",
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Rewind to access your camera to capture moments.",
          "microphonePermission": false
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "15.1",
            "buildReactNativeFromSource": true
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/perf",
      "./plugins/withFirebaseFix",
      "@giphy/react-native-sdk",
      "expo-audio"
    ],
    "owner": "spoodsjs",
    "extra": {
      "eas": {
        "projectId": "d604ae31-6cc9-4459-9b96-cf3d84f85099"
      }
    }
  }
}
