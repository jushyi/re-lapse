{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "GOOGLE_SERVICES_PLIST": "./GoogleService-Info.plist"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "ios": {
        "simulator": false
      },
      "env": {
        "GOOGLE_SERVICES_PLIST": "./GoogleService-Info.plist"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "env": {
        "APP_ENV": "production",
        "GOOGLE_SERVICES_PLIST": "./GoogleService-Info-prod.plist"
      },
      "ios": {
        "image": "latest"
      }
    },
    "production-internal": {
      "extends": "production",
      "distribution": "internal"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6759178451"
      }
    }
  }
}
